{
  "name": "resonance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npx vite",
    "build": "npx tsc && npx vite build",
    "check": "npx tsc --noEmit",
    "lint": "npx eslint .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:fuzz": "npx playwright test --project=chromium",
    "test:fuzz:headed": "npx playwright test --project=chromium --headed",
    "test:fuzz:quick": "FUZZ_DURATION=30 npx playwright test --project=chromium fuzz.spec.ts",
    "test:fuzz:long": "FUZZ_DURATION=300 npx playwright test --project=chromium fuzz.spec.ts",
    "format": "npx prettier --write ."
  },
  "dependencies": {
    "scenerystack": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@vitest/coverage-v8": "^4.0.18",
    "canvas": "^3.2.0",
    "eslint": "^9.39.2",
    "globals": "^15.15.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
